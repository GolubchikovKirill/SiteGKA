{
  "id": null,
  "uid": "infrascope-ml",
  "title": "infrascope-ml",
  "tags": ["infrascope", "ml"],
  "timezone": "browser",
  "schemaVersion": 39,
  "version": 1,
  "refresh": "30s",
  "panels": [
    {
      "type": "timeseries",
      "title": "ML Train Runs",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 0 },
      "targets": [
        { "expr": "sum(rate(infrascope_ml_train_runs_total[5m])) by (model_family, result)", "legendFormat": "{{model_family}} {{result}}" }
      ]
    },
    {
      "type": "timeseries",
      "title": "ML Inference Duration p95",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 0 },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(infrascope_ml_inference_duration_seconds_bucket[5m])) by (le, operation))",
          "legendFormat": "{{operation}}"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "ML Predictions per Minute",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 8 },
      "targets": [
        { "expr": "sum(rate(infrascope_ml_predictions_total[1m])) by (prediction_kind, risk_level)", "legendFormat": "{{prediction_kind}} {{risk_level}}" }
      ]
    },
    {
      "type": "stat",
      "title": "Active ML Models",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 8 },
      "targets": [
        { "expr": "sum(infrascope_ml_model_active_info)", "legendFormat": "active models" }
      ],
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "orientation": "auto",
        "textMode": "value"
      }
    }
  ],
  "templating": { "list": [] },
  "time": { "from": "now-6h", "to": "now" },
  "timepicker": {},
  "annotations": { "list": [] }
}
